import matplotlib.pyplot as plt
from matplotlib.backends.backend_pdf import PdfPages

base = [4575769, 748687, 371429, 263886, 224024]

x_tics = [1, 2, 3, 4, 5]

plt.figure(num=None, figsize=(6, 3), dpi=80, facecolor='w', edgecolor='k')
plt.plot(base, marker="o", linestyle="-", color='darkblue')
plt.yscale('log')
plt.xticks([0, 1, 2, 3, 4], x_tics)
plt.ylabel("Appends/s")
plt.xlabel("Colors per append")
pp = PdfPages('tmp.pdf')
pp.savefig(bbox_inches='tight')
pp.close()
plt.show()
